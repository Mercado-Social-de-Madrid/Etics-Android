<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="net.mercadosocial.moneda.ui.intro.IntroLastItemFragment"
    android:paddingLeft="40dp"
    android:paddingRight="40dp">

    <TextView
        style="@style/TextBase.Purple"
        android:text="@string/start"
        android:textSize="@dimen/text_large"/>

    <net.mercadosocial.moneda.views.SelectMESView
        android:id="@+id/select_mes_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/btn_intro_enter"
        style="@style/ButtonMes"
        android:text="@string/go_to_entities_list"
        android:layout_marginTop="10dp"
        android:enabled="false"/>


</LinearLayout>
